{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://nirvanchitnis-cmyk.github.io/schemas/def14a.schema.json",
  "title": "Bank DEF14A Canonical Factbase",
  "type": "object",
  "required": [
    "version",
    "generated_at_utc",
    "company",
    "filing",
    "board",
    "ownership",
    "compensation",
    "equity_plan",
    "audit",
    "provenance"
  ],
  "properties": {
    "version": {
      "type": "string",
      "const": "1.0.0"
    },
    "generated_at_utc": {
      "type": "string",
      "format": "date-time"
    },
    "company": {
      "type": "object",
      "required": [
        "name",
        "ticker",
        "cik"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "ticker": {
          "type": "string"
        },
        "exchange": {
          "type": "string"
        },
        "cik": {
          "type": "string",
          "pattern": "^[0-9]{10}$"
        },
        "rssd": {
          "type": [
            "string",
            "null"
          ]
        },
        "fiscal_year_end": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        }
      }
    },
    "filing": {
      "type": "object",
      "required": [
        "form",
        "accession_number",
        "filing_date",
        "edgar_urls"
      ],
      "properties": {
        "form": {
          "type": "string",
          "enum": [
            "DEF 14A",
            "DEFA14A",
            "PRE 14A",
            "DEFN 14A"
          ]
        },
        "accession_number": {
          "type": "string",
          "pattern": "^[0-9-]+$"
        },
        "filing_date": {
          "type": "string",
          "format": "date"
        },
        "period_of_report": {
          "type": [
            "string",
            "null"
          ],
          "format": "date"
        },
        "meeting_date": {
          "type": [
            "string",
            "null"
          ],
          "format": "date"
        },
        "meeting_type": {
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "annual",
            "special",
            null
          ]
        },
        "edgar_urls": {
          "type": "object",
          "required": [
            "index",
            "primary_doc"
          ],
          "properties": {
            "index": {
              "type": "string",
              "format": "uri"
            },
            "primary_doc": {
              "type": "string",
              "format": "uri"
            },
            "pdf_fallback": {
              "type": [
                "string",
                "null"
              ],
              "format": "uri"
            }
          }
        },
        "is_scanned_pdf": {
          "type": "boolean"
        }
      }
    },
    "peer_group": {
      "type": "object",
      "properties": {
        "source": {
          "type": "string",
          "enum": [
            "proxy_disclosed",
            "analyst_selected",
            "mixed"
          ]
        },
        "peers": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "name",
              "ticker"
            ],
            "properties": {
              "name": {
                "type": "string"
              },
              "ticker": {
                "type": "string"
              },
              "cik": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "weight_pct": {
                "type": [
                  "number",
                  "null"
                ],
                "minimum": 0,
                "maximum": 100
              }
            }
          }
        }
      }
    },
    "board": {
      "type": "object",
      "required": [
        "size",
        "independent_count",
        "leadership"
      ],
      "properties": {
        "size": {
          "type": "integer",
          "minimum": 1
        },
        "independent_count": {
          "type": "integer",
          "minimum": 0
        },
        "classified_board": {
          "type": "boolean"
        },
        "election_standard": {
          "type": "string",
          "enum": [
            "majority",
            "plurality",
            "plurality_plus_resign",
            "unknown"
          ]
        },
        "refresh_rate_pct": {
          "type": [
            "number",
            "null"
          ],
          "minimum": 0,
          "maximum": 100
        },
        "avg_tenure_years": {
          "type": [
            "number",
            "null"
          ],
          "minimum": 0
        },
        "skills_matrix": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "committees": {
          "type": "object",
          "properties": {
            "audit": {
              "type": "boolean"
            },
            "risk": {
              "type": "boolean"
            },
            "compensation": {
              "type": "boolean"
            },
            "nominating": {
              "type": "boolean"
            },
            "financial_expert_present": {
              "type": "boolean"
            }
          }
        },
        "leadership": {
          "type": "object",
          "required": [
            "chair_is_independent",
            "ceo_is_chair"
          ],
          "properties": {
            "chair_is_independent": {
              "type": "boolean"
            },
            "ceo_is_chair": {
              "type": "boolean"
            },
            "lead_independent_director": {
              "type": "boolean"
            },
            "lead_independent_director_powers": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "ownership": {
      "type": "object",
      "required": [
        "top5_pct",
        "insider_pct",
        "policies"
      ],
      "properties": {
        "top_holders": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "holder",
              "pct"
            ],
            "properties": {
              "holder": {
                "type": "string"
              },
              "pct": {
                "type": "number",
                "minimum": 0,
                "maximum": 100
              }
            }
          }
        },
        "top5_pct": {
          "type": "number",
          "minimum": 0,
          "maximum": 100
        },
        "insider_pct": {
          "type": "number",
          "minimum": 0,
          "maximum": 100
        },
        "policies": {
          "type": "object",
          "properties": {
            "hedging_prohibited": {
              "type": "boolean"
            },
            "pledging_prohibited": {
              "type": "boolean"
            },
            "stock_ownership_guidelines": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    },
    "compensation": {
      "type": "object",
      "required": [
        "pay_mix_pct",
        "annual_incentive",
        "ltip",
        "say_on_pay_pct"
      ],
      "properties": {
        "pay_mix_pct": {
          "type": "object",
          "properties": {
            "base": {
              "type": "number",
              "minimum": 0,
              "maximum": 100
            },
            "cash_bonus": {
              "type": "number",
              "minimum": 0,
              "maximum": 100
            },
            "equity": {
              "type": "number",
              "minimum": 0,
              "maximum": 100
            }
          }
        },
        "neos": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "role",
              "name"
            ],
            "properties": {
              "role": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "total_comp_usd": {
                "type": [
                  "number",
                  "null"
                ]
              },
              "pay_mix_pct": {
                "type": [
                  "object",
                  "null"
                ]
              }
            }
          }
        },
        "annual_incentive": {
          "type": "object",
          "properties": {
            "metrics": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "name",
                  "weight_pct"
                ],
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "weight_pct": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 100
                  },
                  "threshold": {
                    "type": [
                      "number",
                      "string",
                      "null"
                    ]
                  },
                  "target": {
                    "type": [
                      "number",
                      "string",
                      "null"
                    ]
                  },
                  "max": {
                    "type": [
                      "number",
                      "string",
                      "null"
                    ]
                  },
                  "result": {
                    "type": [
                      "number",
                      "string",
                      "null"
                    ]
                  }
                }
              }
            },
            "payout_ratio_pct": {
              "type": [
                "number",
                "null"
              ],
              "minimum": 0
            }
          }
        },
        "ltip": {
          "type": "object",
          "properties": {
            "metrics": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "name",
                  "weight_pct"
                ],
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "weight_pct": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 100
                  },
                  "relative_to_peer_group": {
                    "type": [
                      "boolean",
                      "null"
                    ]
                  }
                }
              }
            },
            "vehicles": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "RSU",
                  "PSU",
                  "options",
                  "cash"
                ]
              }
            }
          }
        },
        "pvp": {
          "type": "object",
          "properties": {
            "years": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "year"
                ],
                "properties": {
                  "year": {
                    "type": "integer"
                  },
                  "cap_usd": {
                    "type": [
                      "number",
                      "null"
                    ]
                  },
                  "tsr_pct": {
                    "type": [
                      "number",
                      "null"
                    ]
                  },
                  "peer_tsr_pct": {
                    "type": [
                      "number",
                      "null"
                    ]
                  },
                  "net_income_usd": {
                    "type": [
                      "number",
                      "null"
                    ]
                  },
                  "csm": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "csm_value": {
                    "type": [
                      "number",
                      "null"
                    ]
                  }
                }
              }
            }
          }
        },
        "say_on_pay_pct": {
          "type": [
            "number",
            "null"
          ],
          "minimum": 0,
          "maximum": 100
        }
      }
    },
    "equity_plan": {
      "type": "object",
      "required": [
        "overhang_pct",
        "burn_rate_pct"
      ],
      "properties": {
        "overhang_pct": {
          "type": [
            "number",
            "null"
          ],
          "minimum": 0
        },
        "burn_rate_pct": {
          "type": [
            "number",
            "null"
          ],
          "minimum": 0
        },
        "shares_available": {
          "type": [
            "number",
            "null"
          ]
        },
        "shares_outstanding_awards": {
          "type": [
            "number",
            "null"
          ]
        },
        "plan_name": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "severance_cic": {
      "type": "object",
      "properties": {
        "ceo_multiple": {
          "type": [
            "number",
            "null"
          ]
        },
        "trigger": {
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "single",
            "double",
            null
          ]
        },
        "gross_up": {
          "type": [
            "boolean",
            "null"
          ]
        }
      }
    },
    "audit": {
      "type": "object",
      "required": [
        "auditor",
        "fees"
      ],
      "properties": {
        "auditor": {
          "type": "string"
        },
        "tenure_years": {
          "type": [
            "number",
            "null"
          ]
        },
        "fees": {
          "type": "object",
          "properties": {
            "audit": {
              "type": [
                "number",
                "null"
              ]
            },
            "audit_related": {
              "type": [
                "number",
                "null"
              ]
            },
            "tax": {
              "type": [
                "number",
                "null"
              ]
            },
            "all_other": {
              "type": [
                "number",
                "null"
              ]
            },
            "non_audit_pct": {
              "type": [
                "number",
                "null"
              ],
              "minimum": 0,
              "maximum": 100
            }
          }
        }
      }
    },
    "proposals": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "title",
          "management_or_shareholder",
          "support_pct"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "management_or_shareholder": {
            "type": "string",
            "enum": [
              "management",
              "shareholder"
            ]
          },
          "support_pct": {
            "type": [
              "number",
              "null"
            ],
            "minimum": 0,
            "maximum": 100
          },
          "status": {
            "type": [
              "string",
              "null"
            ],
            "enum": [
              "passed",
              "failed",
              "N/A",
              null
            ]
          }
        }
      }
    },
    "related_party": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "policies": {
      "type": "object",
      "properties": {
        "clawback": {
          "type": [
            "string",
            "null"
          ]
        },
        "anti_hedging": {
          "type": [
            "string",
            "null"
          ]
        },
        "anti_pledging": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "metrics_derived": {
      "type": "object",
      "properties": {
        "pay_intensity_pct_of_net_income": {
          "type": [
            "number",
            "null"
          ]
        },
        "ownership_concentration_top5_pct": {
          "type": [
            "number",
            "null"
          ]
        }
      }
    },
    "footnotes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "text": {
            "type": "string"
          }
        }
      }
    },
    "provenance": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "section",
          "method",
          "locators",
          "confidence_pct"
        ],
        "properties": {
          "section": {
            "type": "string"
          },
          "method": {
            "type": "string",
            "enum": [
              "html_parse",
              "pdf_text",
              "table_detect",
              "ocr",
              "regex",
              "ml_ner"
            ]
          },
          "locators": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "page_range": {
            "type": [
              "string",
              "null"
            ]
          },
          "text_snippet": {
            "type": [
              "string",
              "null"
            ]
          },
          "confidence_pct": {
            "type": "number",
            "minimum": 0,
            "maximum": 100
          },
          "source_url": {
            "type": [
              "string",
              "null"
            ],
            "format": "uri"
          },
          "sha256": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      }
    }
  },
  "additionalProperties": false
}
